{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:clean && npm run build:compile && npm run build:move",
    "build:clean": "rm -rf static/dist/*",
    "build:compile": "vite build",
    "build:move": "cp build/* static/dist/",
    "lint": "eslint .",
    "dev2": "NODE_ENV=development node watch.js & python app.py",
    "preview": "vite preview",
    "db:new": "npx dbmate -d \"db/migrations\" -e \"DB\" new ",
    "db:up": "npx dbmate -d \"db/migrations\" -e \"DB\" up",
    "db:rollback": "npx dbmate -d \"db/migrations\" -e \"DB\" rollback"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.7",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "chokidar": "^4.0.3",
    "dbmate": "^2.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "sqlite3": "^5.1.7",
    "vite": "^6.0.5"
  }
}
